<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
        <record id="students_sequence_id" model="ir.sequence">
            <field name="name">Students sequence</field>
            <field name="code">school.students</field>
            <field name="prefix">STD</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="students_admission_no" model="ir.sequence">
            <field name="name">Students admission no</field>
            <field name="code">school.students.admission</field>
            <field name="prefix">ADM</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="company_id" eval="False"/>
        </record>
</odoo>