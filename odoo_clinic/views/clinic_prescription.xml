<?xml version="1.0" encoding="utf-8"?>
    <odoo>
            <record id="clinic_prescription_action" model="ir.actions.act_window">
                <field name="name">Clinic Prescription</field>
                <field name="res_model">clinic.prescription</field>
                <field name="view_mode">list,form</field>
            </record>

            <record id="clinic_prescription_list_view" model="ir.ui.view">
                <field name="name">clinic.prescription.view</field>
                <field name="model">clinic.prescription</field>
                <field name="arch" type="xml">
                    <list string="Tests" edit="True" editable="bottom" open_form_view="True">
<!--                        <field name="patient_name"/>-->
                        <field name="medicine"/>
                        <field name="quantity"/>
                        <field name="dosage"/>
                    </list>
                </field>
            </record>

    </odoo>