<?xml version="1.0" encoding="utf-8"?>
    <odoo>
            <record id="clinic_consultation_action" model="ir.actions.act_window">
                <field name="name">Clinic Consultation</field>
                <field name="res_model">clinic.consultation</field>
                <field name="view_mode">list,form</field>
            </record>

            <record id="clinic_consultation_list_view" model="ir.ui.view">
                <field name="name">clinic.consultation.view</field>
                <field name="model">clinic.consultation</field>
                <field name="arch" type="xml">
                    <list string="Tests" multi_edit="1" >
                        <field name="patient_name"/>
                        <field name="doctor_name"/>
                        <field name="serial_no"/>
                    </list>
                </field>
            </record>

    </odoo>